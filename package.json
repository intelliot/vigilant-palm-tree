{
  "name": "xrpl-developer-program-website",
  "version": "0.1.3",
  "description": "XRPL Grants Program Website",
  "author": "Javi <javi@ripple.com>",
  "license": "MIT",
  "scripts": {
    "build": "genversion lib/version.js && vue-cli-service build && cp dist/index.html dist/404.html",
    "lint": "vue-cli-service lint",
    "changelog": "conventional-changelog --config ./commits.config.js -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog --config ./commits.config.js -i CHANGELOG.md -s -r 0",
    "dev": "NODE_ENV=development vue-cli-service serve",
    "deploy:ghp": "GIT_DEPLOY_REPO=https://github.com/xpring-eng/xrpl-developer-program-website.git lib/deploy.sh"
  },
  "dependencies": {
    "ua-parser-js": "^0.7.21",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.18.2",
    "vue-localstorage": "^0.6.2",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.1.6",
    "vuelidate-property-decorators": "^1.0.28",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/vuelidate": "^0.7.13",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/cli-plugin-babel": "~4.2.3",
    "@vue/cli-plugin-eslint": "~4.2.3",
    "@vue/cli-plugin-typescript": "^4.5.9",
    "@vue/cli-service": "~4.2.3",
    "@vue/eslint-config-standard": "^5.1.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "^1.1.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "genversion": "^2.2.1",
    "husky": "^4.3.0",
    "node-sass": "^4.13.1",
    "raw-loader": "^4.0.2",
    "sass-loader": "^8.0.2",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "vue-template-compiler": "^2.6.11"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
